{
    "version": 2,
    "env": {
        "MONGO_URI": "MONGODB_URI"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ],
    "headers": [
        {
            "source": "/(*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://codeverse-front.vercel.app/"
                }
            ]
        }
    ]
}